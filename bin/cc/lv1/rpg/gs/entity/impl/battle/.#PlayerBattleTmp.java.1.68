				long b111 = System.currentTimeMillis();
//				System.out.println(skill.name+ " PlayerBattleTmp 技能产生---持续效果 : "+effect.name+"  目标范围:"+skill.effectRangeList);
				TimeEffect te = (TimeEffect)effect;
