				long t112 = System.currentTimeMillis();
				//打除了主目标以外的怪
				if(skill.effectRangeList != ActiveSkill.EFFECT_RANGE_ONE)
				{
					int targetType = getSkillTargeType();
		
					if(skill.targetType[i] == ActiveSkill.TARGET_TYPE_PLAYERS)
